#include <stdio.h>
struct Student { char name[50]; int age; };
int main() {
struct Student s={"Alice",20};
FILE *fp=fopen("data.bin","wb"); fwrite(&s,sizeof(s),1,fp); fclose(fp);
struct Student r; fp=fopen("data.bin","rb"); fread(&r,sizeof(r),1,fp);
fclose(fp);
printf("Name: %s Age: %d\n", r.name, r.age);
return 0;
}
