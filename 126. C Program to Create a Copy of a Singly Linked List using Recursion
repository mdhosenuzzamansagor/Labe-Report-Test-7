#include <stdio.h>
#include <stdlib.h>
struct Node { int data; struct Node *next; };
struct Node* copyList(struct Node *head){ if(!head) return NULL; struct Node
*newNode=(struct Node*)malloc(sizeof(struct Node)); newNode->data=head->data;
newNode->next=copyList(head->next); return newNode; }
int main() {
struct Node *head=(struct Node*)malloc(sizeof(struct Node)); head-
>data=1; head->next=NULL;
struct Node *copy=copyList(head);
printf("%d\n", copy->data); 
return 0; 
}
