#include <stdio.h>
#include <string.h>
int main() {
char a[100], b[100];
scanf("%s %s", a, b);
int n1 = strlen(a), n2 = strlen(b), carry = 0, sum;
char result[101];
int k = 0;
for(int i=n1-1,j=n2-1;i>=0||j>=0;i--,j--,k++) {
int bit1 = (i>=0)? a[i]-'0' : 0;
int bit2 = (j>=0)? b[j]-'0' : 0;
sum = bit1 + bit2 + carry;
result[k] = (sum%2)+'0';
carry = sum/2;
}
if(carry) result[k++]='1';
for(int i=k-1;i>=0;i--) printf("%c", result[i]);
return 0;
}
